<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tarena.mnmp.passport.dao.UserMapper">


    <insert id="save" useGeneratedKeys="true" keyProperty="id" parameterType="com.tarena.mnmp.passport.dao.RoleDO">
        insert into notice_role (name,create_time,update_time)
        values (#{name},now(),now());
    </insert>
</mapper>

